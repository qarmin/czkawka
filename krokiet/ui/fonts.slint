export global FontSizes {
    // Main window title
    out property <length> title: 22px;
    // Section headers
    out property <length> header: 17px;
    // Default/normal text
    out property <length> normal: 13px;
    // Small text (hints, secondary)
    out property <length> small: 12px;

    out property <int> bold_weight: 600;
    out property <int> bold_light: 500;
}
